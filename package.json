{
  "name": "localize-backend",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "deploy": "wrangler deploy",
    "dev": "wrangler dev",
    "start": "wrangler dev",
    "test:integration": "node --test --env-file=.dev.vars --test-name-pattern=\"integration test\"",
    "test:unit": "node --test --test-name-pattern=\"unit test\"",
    "test:unit:watch": "node --test --watch --test-name-pattern=\"unit test\"",
    "test:unit:coverage": "node --test --experimental-test-coverage --test-name-pattern=\"unit test\"",
    "prettier": "prettier --write . --ignore-path .lintignore",
    "prettier:check": "prettier --check . --ignore-path .lintignore",
    "eslint": "eslint . --fix --ignore-path .lintignore",
    "eslint:check": "eslint . --ignore-path .lintignore",
    "lint": "npm run prettier && npm run eslint",
    "lint:check": "npm run prettier:check && npm run eslint:check"
  },
  "devDependencies": {
    "@babel/eslint-parser": "7.22.15",
    "@babel/plugin-syntax-import-assertions": "7.22.5",
    "eslint": "8.52.0",
    "eslint-config-prettier": "9.0.0",
    "prettier": "3.0.3",
    "wrangler": "^3.0.0"
  },
  "dependencies": {
    "lodash": "4.17.21"
  }
}
